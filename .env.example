# CodeJam Backend Environment Variables
# Copy this file to .env and fill in your values
# NEVER commit .env to git!

# ==================== GHCR ====================
GHCR_OWNER=codejam-dev
IMAGE_TAG=latest

# ==================== DATABASE ====================
POSTGRES_DB=codejam_db
POSTGRES_USER=codejam
POSTGRES_PASSWORD=

# ==================== JWT ====================
# Generate with: openssl rand -base64 64
JWT_SECRET=

# ==================== URLS ====================
FRONTEND_URL=https://frontentUrl.app
GATEWAY_BASE_URL=http://<backendurl>:8080

# ==================== GOOGLE OAUTH ====================
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# ==================== EMAIL (SMTP) ====================
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=
MAIL_PASSWORD=

# ==================== CONFIG SERVER ====================
CONFIG_REPO_URL=https://github.com/codejam-dev/config-repo.git
GIT_USERNAME=
GIT_PASSWORD=

# ==================== OPTIONS ====================
OTP_ENABLE_DYNAMIC=true
